<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/resume_edit_parent"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:fitsSystemWindows="true"
    android:orientation="vertical" >

    <FrameLayout
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#22b570" >

        <ImageView
            android:id="@+id/back"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="10dp"
            android:src="@drawable/back"
            android:visibility="visible" />

        <TextView
            android:id="@+id/title_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:text="个人简历"
            android:textColor="#ffffff"
            android:textSize="17sp" />

        <TextView
            android:id="@+id/title_tv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|right"
            android:layout_marginRight="15dp"
            android:paddingBottom="10dp"
            android:text="保存"
            android:textColor="#ffffff"
            android:textSize="15sp"
            android:visibility="visible" />
    </FrameLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="none" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical" >

            <!-- 照片 -->

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:paddingBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingTop="10dp" >

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <LinearLayout
                        android:id="@+id/container"
                        android:layout_width="wrap_content"
                        android:layout_height="60dp"
                        android:orientation="horizontal" >
                    </LinearLayout>

                    <ImageView
                        android:id="@+id/add_user_photo"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:src="@drawable/add_photo" />
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|right"
                    android:layout_marginRight="10dp"
                    android:text="可添加3张图片\n(长按可删除)"
                    android:textColor="#a1a1a1"
                    android:textSize="10sp" />
            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:text="基本信息"
                    android:textColor="#555555"
                    android:textSize="10sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:paddingBottom="5dp"
                    android:paddingLeft="5dp"
                    android:paddingTop="5dp"
                    android:text="(带 *项为必填项)"
                    android:textColor="#555555"
                    android:textSize="8sp" />
            </LinearLayout>

            <!-- 基本信息 -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:orientation="vertical" >

                <!-- 姓名 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="姓名*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <EditText
                        android:id="@+id/tv_resume_name"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请输入真实姓名"
                        android:paddingRight="20dp"
                        android:textColor="#a1a1a1"
                        android:textColorHint="#a1a1a1"
                        android:textSize="11sp" />
                </LinearLayout>

                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 性别 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="性别*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <LinearLayout
                            android:id="@+id/title_right"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_gravity="right"
                            android:layout_marginRight="23dp"
                            android:background="@drawable/sex_choose"
                            android:gravity="center_vertical"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/tv_resume_sexman"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/sex_man_choose1"
                                android:gravity="center"
                                android:paddingBottom="2dp"
                                android:paddingLeft="25dp"
                                android:paddingRight="25dp"
                                android:paddingTop="2dp"
                                android:text="男"
                                android:textColor="@color/white"
                                android:textSize="14sp" />

                            <TextView
                                android:id="@+id/tv_resume_sexwoman"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:background="@drawable/sex_woman_choose2"
                                android:gravity="center"
                                android:paddingBottom="2dp"
                                android:paddingLeft="25dp"
                                android:paddingRight="25dp"
                                android:paddingTop="2dp"
                                android:text="女"
                                android:textColor="#22b570"
                                android:textSize="14sp" />
                        </LinearLayout>
                    </FrameLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 出生年月 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="出生年月*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fv_resume_birth"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_birth"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="请选择"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>
                </LinearLayout>

                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 身高体重 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:weightSum="4" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="身高(cm)"
                        android:textColor="#101119"
                        android:textSize="11sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="3" >

                        <EditText
                            android:id="@+id/tv_resume_height"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:background="@null"
                            android:gravity="center"
                            android:hint="点击输入"
                            android:maxLength="3"
                            android:numeric="integer"
                            android:textColor="#a1a1a1"
                            android:textColorHint="#a1a1a1"
                            android:textSize="11sp" />
                        <!-- divider -->

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#eeeeee" />

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="体重(kg)"
                            android:textColor="#101119"
                            android:textSize="11sp" />
                        <!-- divider -->

                        <TextView
                            android:layout_width="1dp"
                            android:layout_height="match_parent"
                            android:background="#eeeeee" />

                        <EditText
                            android:id="@+id/tv_resume_weight"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_marginBottom="10dp"
                            android:layout_marginTop="10dp"
                            android:layout_weight="0.99"
                            android:background="@null"
                            android:gravity="center"
                            android:hint="点击输入"
                            android:maxLength="3"
                            android:numeric="integer"
                            android:textColor="#a1a1a1"
                            android:textColorHint="#a1a1a1"
                            android:textSize="11sp" />
                    </LinearLayout>
                </LinearLayout>
                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 特长 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="特长*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <EditText
                        android:id="@+id/tv_resume_speciality"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:background="@null"
                        android:gravity="right"
                        android:hint="请输入特长"
                        android:paddingRight="20dp"
                        android:textColor="#a1a1a1"
                        android:textColorHint="#a1a1a1"
                        android:textSize="11sp" />
                </LinearLayout>
                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 职业 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="职业*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fv_resume_occupation"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_occupation"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="填写现在职业"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />
                <!-- 现住地 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="现住地*"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fv_resume_address"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_address"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="请填写现住地址"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>
                </LinearLayout>
            </LinearLayout>
            <!-- 基本信息end -->

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingBottom="5dp"
                android:paddingLeft="10dp"
                android:paddingTop="5dp"
                android:text="教育信息"
                android:textColor="#555555"
                android:textSize="10sp" />
            <!-- 教育信息 -->

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="#ffffff"
                android:orientation="vertical" >

                <!-- 最高学历 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="最高学历"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fv_resume_education"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_education"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="点击选择"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>
                </LinearLayout>

                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 毕业院校 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="毕业院校"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fl_resume_school"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_school"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="点击选择"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>
                </LinearLayout>

                <!-- divider -->

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#eeeeee" />

                <!-- 技能证书 -->

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_weight="4"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="技能证书"
                        android:textColor="#101119"
                        android:textSize="12sp" />

                    <TextView
                        android:layout_width="1dp"
                        android:layout_height="match_parent"
                        android:background="#eeeeee" />

                    <FrameLayout
                        android:id="@+id/fv_resume_skill"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="10dp"
                        android:layout_marginTop="10dp"
                        android:layout_weight="3"
                        android:orientation="horizontal" >

                        <ImageView
                            android:layout_width="15dp"
                            android:layout_height="15dp"
                            android:layout_gravity="right"
                            android:layout_marginRight="15dp"
                            android:src="@drawable/right_arrow" />

                        <TextView
                            android:id="@+id/tv_resume_skill"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center_vertical"
                            android:layout_marginRight="33dp"
                            android:gravity="right"
                            android:text="请尽可能填写所获得的有效证书"
                            android:textColor="#a1a1a1"
                            android:textSize="11sp" />
                    </FrameLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="vertical"
                        android:visibility="invisible" >

                        <TextView
                            android:id="@+id/skillTextOne"
                            android:layout_width="0dp"
                            android:layout_height="0dp" />

                        <ImageView
                            android:id="@+id/skillImgOne"
                            android:layout_width="0dp"
                            android:layout_height="0dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="vertical"
                        android:visibility="invisible" >

                        <TextView
                            android:id="@+id/skillTextTwo"
                            android:layout_width="0dp"
                            android:layout_height="0dp" />

                        <ImageView
                            android:id="@+id/skillImgTwo"
                            android:layout_width="0dp"
                            android:layout_height="0dp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:orientation="vertical"
                        android:visibility="invisible" >

                        <TextView
                            android:id="@+id/skillTextThree"
                            android:layout_width="0dp"
                            android:layout_height="0dp" />

                        <ImageView
                            android:id="@+id/skillImgThree"
