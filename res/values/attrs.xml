<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RoundImageView">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color" />
            <attr name="borderRadius" format="dimension" />
            <attr name="type">
                <enum name="circle" value="0" />
                <enum name="round" value="1" />
            </attr>
    </declare-styleable>
    <declare-styleable name="WaterDropView">
        <attr name="max_circle_radius" format="dimension" />
        <attr name="min_circle_radius" format="dimension" />
        <attr name="waterdrop_color" format="color" />
        <attr name="topcircle_x" format="dimension" />
        <attr name="topcircle_y" format="dimension" />
        <attr name="bottomcircle_x" format="dimension" />
        <attr name="bottomcircle_y" format="dimension" />
    </declare-styleable>
    <declare-styleable name="FixLinerLayout">
        <attr name="leftPadding" format="integer" />
        <attr name="topPadding" format="integer" />
        <attr name="maxCount" format="integer" />
        <attr name="mHDivider" format="integer" />
        <attr name="mWDivider" format="integer" />
    </declare-styleable>

</resources>